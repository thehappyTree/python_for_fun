    .mod .album-item {
        padding: 20px 0;
        margin: 0;
        border-bottom: 1px dashed #E8E8E8;
    }
    .album-item .meta,
    .note-item .meta {
        margin-top: 10px;
    }
  </style>
    <script></script>

