var Stream = function (_React$Component3) {
    _inherits(Stream, _React$Component3);

    function Stream(props) {
        _classCallCheck(this, Stream);

        var _this4 = _possibleConstructorReturn(this, (Stream.__proto__ || Object.getPrototypeOf(Stream)).call(this, props));

        _this4.state = {
            stream: [],
            total: null,
            start: 0,
            doing: false,
            is_fold: false
        };
        return _this4;
    }

    _createClass(Stream, [{
        key: 'componentWillMount',
        value: function componentWillMount() {
            this.load(window.topic_display_count);
        }
    }, {
        key: 'load',
        value: function load(count) {
            var _this5 = this;

            if (this.state.doing) return;
            this.setState({ doing: true });
            if (count === 0) return;
            $.ajax({
                url: 'https://m.douban.com/rexxar/api/v2/gallery/subject_feed?start=' + this.state.start + '&count=' + count + '&subject_id=' + window.subject_id + '&ck=' + get_cookie('ck'),
                xhrFields: { withCredentials: true },
                success: function success(res) {
                    _this5.setState({
                        stream: _this5.state.stream.concat(res.items),
                        total: res.total,
                        start: _this5.state.start + count,
                        doing: false
                    });
                    if (res.total === 0 && window.no_content_fun_call_name !== '') {
                        window[window.no_content_fun_call_name]();
                    }
                    $('#topic-count').text(res.total);
                }
            });
        }
    }, {
        key: 'fold',
        value: function fold(last_state) {
            this.setState({ is_fold: !last_state });
        }
    }, {
        key: 'render',
        value: function render() {
            var _state = this.state,
                total = _state.total,
                stream = _state.stream,
                is_fold = _state.is_fold;

            return React.createElement(
                'div',
                { className: 'react-root', 'data-fold': is_fold },
                stream.map(function (e, i) {
                    return React.createElement(Topic, { key: i, topic: e });
                }),
                total && total > window.topic_display_count && total !== stream.length ? React.createElement(
                    'div',
                    { className: 'topic-more', onClick: this.load.bind(this, total - this.state.start) },
                    '( ',
                    React.createElement(
                        'a',
                        { href: 'javascript:;;' },
                        '\u5C55\u5F00\u5168\u90E8\u8BDD\u9898'
                    ),
                    ' )'
                ) : null,
                total && total > window.topic_display_count && total === stream.length ? React.createElement(
                    'div',
                    { className: 'topic-more', onClick: this.fold.bind(this, is_fold) },
                    '( ',
                    React.createElement(
                        'a',
                        { href: 'javascript:;;' },
                        is_fold ? '展开全部话题' : '收起话题'
                    ),
                    ' )'
                ) : null
            );
        }
    }]);

    return Stream;
}(React.Component);

ReactDOM.render(React.createElement(Stream, null), document.getElementById('topic-items'));;
    })();
</script><script type="text/javascript" src="https://img3.doubanio.com/misc/mixed_static/197a597815da8737.js"></script>
